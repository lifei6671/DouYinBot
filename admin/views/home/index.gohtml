<script type="application/javascript">
    function getVideoContent() {
        $("#errorMessage").text('');
        $("#alertMessage").text("");
        $("#video-content").html('');
        let douYinContent = $("#douYinContent").val();
        if (douYinContent === ""){
            $("#errorMessage").text('请将分享的内容粘贴到文本框');
            return false;
        }
        const urls = extractUrls(douYinContent);
        return urls.length ? urls[0] : null;
    }

    /**
     * 从文本中提取 URL（http/https），默认返回去重后的数组。
     * - 会剔除常见的中文/英文结尾标点（避免把 "https://xxx/！" 的 "！" 带进去）
     * - 会优先把 douyin.com 的链接排在前面
     */
    function extractUrls(text) {
        if (typeof text !== "string") return [];

        // 匹配 http(s) URL，尽量宽松但不跨空白
        const urlRegex = /https?:\/\/[^\s]+/gi;
        const matches = text.match(urlRegex) || [];

        const cleaned = matches.map(u =>
            u.replace(/[)\]}",'，。！？；：、】【《》“”‘’…]+$/g, "") // 去掉末尾常见标点/括号
        );

        // 去重（保持顺序）
        const seen = new Set();
        const unique = [];
        for (const u of cleaned) {
            if (!seen.has(u)) {
                seen.add(u);
                unique.push(u);
            }
        }

        // douyin 链接优先
        unique.sort((a, b) => {
            const ad = /douyin\.com/i.test(a) ? 0 : 1;
            const bd = /douyin\.com/i.test(b) ? 0 : 1;
            return ad - bd;
        });

        return unique;
    }

    function downVideo (url, name){
        let xhr = new XMLHttpRequest();
        xhr.open('GET', url, true);
        xhr.responseType = 'blob';
        xhr.onload = function () {
            if (xhr.readyState === 4 && xhr.status === 200) {
                let blob = this.response;
                let u = window.URL.createObjectURL(new Blob([blob]));
                let a = document.createElement('a');
                a.download = name;
                a.href = u;
                a.style.display = 'none';
                document.body.appendChild(a);
                a.click();
                a.remove();
            }
        };
        xhr.send()
    }

    $(function (){
        let $alertMessage = $("#alertMessage");
        let $errorMessage = $("#errorMessage");
        let $douYinContent = $("#douYinContent");
        let $form = $("form");

        $form.submit(function (event){
           event.preventDefault();
           let douYinContent = getVideoContent();
           if (douYinContent === false){
               return false;
           }
           $.ajax({
               url:$form.attr("action"),
               dataType:"json",
               type:"post",
               data: $form.serializeArray(),
               success: function ($resp){
                   if($resp.errcode === 0){
                       $("#video-content").html($resp.data);
                   } else {
                       $errorMessage.text($resp.message);
                   }
               },
               error : function (){
                   $errorMessage.text('服务器异常');
               }
           });
          return false;
       }) ;
       $("#downloadVideo").on("click",function (){
           let douYinContent = getVideoContent();
           if (douYinContent === false){
               return false;
           }
            $.ajax( {
               url :"{{urlfor "HomeController.Download"}}" ,
                dataType: "json",
                type:"get",
                data: "url=" + encodeURI(douYinContent),
                success: function ($resp) {
                    if($resp.errcode === 0) {
                        downVideo($resp.data.url,$resp.data.name);
                    }else {
                        $errorMessage.text($resp.message);
                    }
                },
                error : function () {
                    $errorMessage.text('服务器异常');
                }
            });
       });
        $("#sendVideo").on("click", function () {
            let douYinContent = getVideoContent();
            if (douYinContent === false) {
                return false;
            }
            console.log(douYinContent);
            $.ajax({
                url :"{{urlfor "HomeController.SendVideo"}}" ,
                dataType: "json",
                type:"get",
                data: "url=" + encodeURI(douYinContent),
                success: function ($resp) {
                    if($resp.errcode === 0) {
                        $("#douYinContent").val('');
                        $alertMessage.text($resp.message);
                    }else {
                        $errorMessage.text($resp.message);
                    }
                },
                error : function () {
                    $errorMessage.text('服务器异常');
                }
            });
       });
    });
</script>
